            <div class="contaner py-4 text-center">
                <div class="p-3 row">
                    <div class="col-md-6 col-sm-12">
                        <img src="/assets/error.jpg" alt="error_img"
                        class="w-75" />
                    </div>
                    <div class="col-md-6 col-sm-12 align-self-center">
                        <h2 class="p-3 text-success">Error Code  <%=code%></h2>
                        <hr>
                        <div class="text-center">
                            <%=failed%>
                        </div>
                    
                    </div>
                   
                </div>
                <br>
               
            </div>
        </main>
        <!-- Bootstrap JS -->
